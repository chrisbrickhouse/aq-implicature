@article{Potts2015,
abstract = {How do comprehenders reason about pragmatically ambiguous scalar terms like some in complex syntactic contexts? In Gricean theories of conversational implicature, local exhaus- tification of such terms (‘only some') is predicted to be impossible if it does not strengthen the literal meaning, whereas grammatical accounts of implicature predict such construals to be available. Recent experimental evidence supports the availability of these local enrichments, but the grammatical theories that this evidence supports do not provide viable mechanisms for weighting such construals against others. We propose a probabilistic model that combines previous work on pragmatic inference under ‘lexical uncertainty' with a more detailed model of compositional semantics. We show that this model makes accurate predictions about new experimental data on embedded implicatures in both non-monotonic and downward-entailing semantic contexts. In addition, the model's predictions can be improved by the incorporation of neo-Gricean hypotheses about lexical alternatives. This work thus contributes to a synthesis of grammatical and probabilistic views on pragmatic inference.},
author = {Potts, Christopher and Lassiter, Daniel and Levy, Roger and Frank, Michael C},
doi = {10.1093/jos/ffv012},
issn = {0167-5133},
journal = {Journal of Semantics},
keywords = {Bayesian pragmatics,conversational implicatures,embedded implicatures,experimental pragmatics,scalar implicatures},
number = {1975},
pages = {755--802},
title = {{Embedded implicatures as pragmatic inferences under compositional lexical uncertainty}},
volume = {33},
year = {2015}
}
